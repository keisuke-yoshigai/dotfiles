# general alias
alias l='ls -hlAG'
alias ll='ls -hlAG'
alias lll='ls -hlAG'
alias grep='grep --color=auto'
alias vi='vim'
alias less='less -NM'
alias gore='gore -autoimport'

# git alias
alias ga='git add'
alias gb='git branch'
alias gco='git commit'
alias gch='git checkout'
alias gd='git diff'
alias gl='git log'
alias gs='git status'

# export
export GOPATH=$HOME/dev
export GOBIN=$HOME/dev/bin
export PATH=$HOME/anaconda/bin:$PATH:$GOPATH/bin
